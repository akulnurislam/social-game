  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsx scripts/build.ts",
    "start": "node -r dotenv/config dist/index.js",
    "start:api": "node -r dotenv/config dist/index.js api",
    "start:ws": "node -r dotenv/config dist/index.js ws",
    "start:bot": "node -r dotenv/config dist/index.js bot",
    "dev": "tsx --require dotenv/config --watch src/index.ts",
    "migrate": "tsx --require dotenv/config scripts/migrate.ts",
    "client:ws": "tsx --require dotenv/config scripts/ws-client.ts",
    "integration:battle": "tsx --require dotenv/config scripts/integration-battle.ts"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
